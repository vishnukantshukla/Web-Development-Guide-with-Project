Store Procedure:-
create proc logchk
(
    @email varchar(50),
    @pwd varchar(50)
)
as
 declare @ap as varchar(50)
 declare @id as int
 select @id=id, @ap=password from user where Email=@email
 if(@ap is null)
 return -1
 else if(@ap == @pwd )
 return @id
 else
 return -2

 1 a@a.a a
 2 b@b.b b
 3 c@c.c c




